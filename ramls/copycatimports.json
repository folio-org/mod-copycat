{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "tags",
  "description": "copycat import request/response",
  "type": "object",
  "properties": {
    "externalIdentifier": {
      "description": "external identifier for the record",
      "type": "string"
    },
    "internalIdentifier": {
      "description": "internal identifier for the record",
      "type": "string"
    },
    "profileId": {
      "description": "profile to be used for importing",
      "type": "string"
    },
    "record": {
      "description" : "local record",
      "type" : "object",
      "properties" : {
         "json" : {
           "description" : "record object within json property",
           "type" : "object"
          },
         "marc" : {
           "description" : "base64 encoded ISO2709 MARC",
           "type" : "string"
         }
      }
    }
  },
  "additionalProperties": false,
  "required": [ "profileId" ]
}
