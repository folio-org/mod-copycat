{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "tags",
  "description": "copycat import request",
  "type": "object",
  "properties": {
    "externalIdentifier": {
      "description": "external identifier for the record",
      "type": "string"
    },
    "internalIdentifier": {
      "description": "internal identifier for the record",
      "type": "string"
    },
    "targetProfileId": {
      "description": "target profile to be used for importing",
      "type": "string"
    },
    "record": { "type" : "object" }
  },
  "additionalProperties": false,
  "required": [ "targetProfileId" ]
}
